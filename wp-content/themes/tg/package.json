{
  "name": "tg",
  "version": "1.0.0",
  "description": "Wordpress Start Theme",
  "main": "index.js",
  "scripts": {
    "start": "nodemon server.js",
    "watch:scss": "sass --watch src/scss/main.scss:static/css/styles.min.css --style compressed --no-source-map",
    "watch:js": "babel src/js --out-file static/js/scripts.min.js --minified --no-comments --watch",
    "watch": "concurrently \"npm run watch:scss\" \"npm run watch:js\" \"npm run start\""
  },
  "keywords": [
    "timber",
    "wordpress"
  ],
  "author": "Tiago Galv√£o",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "browser-sync": "^2.27.5",
    "cheerio": "*",
    "concurrently": "^7.6.0",
    "http-proxy-middleware": "^1.3.1",
    "nodemon": "^2.0.21",
    "sass": "^1.42.1"
  }
}
